#!/usr/bin/env node

var path = require('path');
var fs   = require('fs');
var root  = path.join(path.dirname(fs.realpathSync(__filename)), '..');

// # GET Command
// 
// Usage:
//
// ``` sh
//  get "http://twitter.com/search.json?q=nodejs"
// ```
require(root + '/index').get(process.argv[2], function(err, data) {
  if (err === null) {
    console.log(data);
  }
});
